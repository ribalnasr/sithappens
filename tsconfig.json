/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "paths": {
      "@sithappens/apis": [
        "dist/sithappens/apis/sithappens-apis",
        "dist/sithappens/apis"
      ],
      "@sithappens/data": [
        "dist/sithappens/data/sithappens-data",
        "dist/sithappens/data"
      ],
      "@sithappens/types": [
        "dist/sithappens/types/sithappens-types",
        "dist/sithappens/types"
      ]
    },
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}